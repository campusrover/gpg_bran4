<launch>

  <include file="$(find raspicam_node)/launch/camerav2_1280x720.launch" />

    <!-- Publish static transform of the camera. Define your sensor offset here. 
         static_transform_publisher x y z yaw pitch roll frame_id child_frame_id -->

  <node pkg="tf" type="static_transform_publisher" name="base_link_to_camera" args="0.11 0.07 0.04  0.0 1.0 0.0   /base_link /raspicam 100" />

</launch>
