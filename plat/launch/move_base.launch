<launch>
    <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
        <rosparam file="$(find platform)/param/navigation/costmap_common_params.yaml" command="load" ns="global_costmap" />
        <rosparam file="$(find platform)/param/navigation/costmap_common_params.yaml" command="load" ns="local_costmap" />
        <rosparam file="$(find platform)/param/navigation/local_costmap_params.yaml" command="load" />
        <rosparam file="$(find platform)/param/navigation/global_costmap_params.yaml" command="load" />
        <rosparam file="$(find platform)/param/navigation/base_local_planner_default_params.yaml" command="load" />
        <rosparam file="$(find platform)/param/navigation/move_base_params.yaml" command="load" />
    </node>
</launch>
